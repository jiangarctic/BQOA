<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ClientMapper">
	<insert id="saveClient" parameterType="pd" flushCache="false">
		insert into sys_client (clientName,clientAddress,clientType,clientContactor,clientPhone,commissionStart,commissionEnd) 
		values (#{clientName} ,#{clientAddress},#{clientType},#{clientContactor},#{clientPhone},#{commissionStart},#{commissionEnd})
	</insert>

</mapper>